{
    "name": "@musicbird/xml-parser",
    "version": "1.1.1",
    "description": "MusicBird - XML Parser",
    "main": "dist/main.js",
    "module": "dist/main.js",
    "typings": "index.d.ts",
    "author": "Fletcher Lind",
    "email": "musicbird.project@gmail.com",
    "license": "GNU GPL",
    "homepage": "https://github.com/MusicBirdProject/musicbird-xml#readme",
